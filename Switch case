#include <stdio.h> 
int main() 
{ 
    int i;
    printf("ENTER THE CHOICE:");
    scanf("%d",&i);
    switch (i) 
        { 
        case 1: 
        printf("ITEM-PIZZA"); 
        printf("\nCOST-RS-239");
        break; 
        case 2: 
        printf("ITEM-BURGER"); 
        printf("\nCOST-RS-129");
        break; 
        case 3: 
        printf("ITEM-PASTA"); 
        printf("\nCOST-RS-179");
        break; 
        case 4: 
        printf("ITEM-FRENCH_FRIES"); 
        printf("\nCOST-RS-99");
        break;
        case 5:
        printf("ITEM-SANDWHICH"); 
        printf("\nCOST-RS-149");
        break;
        
        default: 
        printf("Default "); 
        } 
    
    return 0; 
    
}
